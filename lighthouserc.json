{
  "ci": {
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "aria-required-children": "off",
        "font-display": "off",
        "unused-css-rules": "off",
        "redirects-http": "off",
        "uses-http2": "off"
      }
    },
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "public"
    }
  }
}
